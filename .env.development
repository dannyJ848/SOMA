# Development Environment Configuration
# This file is loaded when running `npm run dev` or `npm run tauri:dev`

# Application
VITE_APP_NAME=SOMA
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_FEATURE_DEBUG_MODE=true
VITE_FEATURE_MOCK_DATA=false
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_ERROR_REPORTING=false

# LLM Configuration
VITE_LLM_DEFAULT_MODEL=qwen2.5-coder-1.5b-instruct-q4_k_m
VITE_LLM_MAX_TOKENS=2048
VITE_LLM_TEMPERATURE=0.7

# Logging
VITE_LOG_LEVEL=debug
VITE_LOG_TO_FILE=true

# Performance
VITE_ENABLE_PROFILING=true
VITE_ENABLE_SOURCE_MAPS=true

# 3D Renderer Settings
VITE_3D_ANTIALIAS=true
VITE_3D_PIXEL_RATIO=2
VITE_3D_MAX_FPS=60
