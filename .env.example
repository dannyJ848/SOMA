# Environment Configuration Example
# Copy this file to .env.local and customize for your needs

# Application
VITE_APP_NAME=Biological Self
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# API Configuration (optional - app works offline by default)
VITE_API_BASE_URL=
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_FEATURE_DEBUG_MODE=false
VITE_FEATURE_MOCK_DATA=false
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_ERROR_REPORTING=false

# LLM Configuration
# Available models: qwen2.5-coder-1.5b-instruct-q4_k_m, llama-3.2-1b-q4_k_m
VITE_LLM_DEFAULT_MODEL=qwen2.5-coder-1.5b-instruct-q4_k_m
VITE_LLM_MAX_TOKENS=2048
VITE_LLM_TEMPERATURE=0.7

# Logging
# Levels: debug, info, warn, error
VITE_LOG_LEVEL=info
VITE_LOG_TO_FILE=false

# Performance
VITE_ENABLE_PROFILING=false
VITE_ENABLE_SOURCE_MAPS=false

# 3D Renderer Settings
VITE_3D_ANTIALIAS=true
VITE_3D_PIXEL_RATIO=1
VITE_3D_MAX_FPS=60

# Optional: Anthropic API Key (for fallback cloud AI)
# ANTHROPIC_API_KEY=sk-ant-...

# Optional: Development signing (macOS)
# APPLE_SIGNING_IDENTITY=Developer ID Application: Your Name
# APPLE_TEAM_ID=XXXXXXXXXX
