# Test Environment Configuration
# This file is loaded when running `npm run test`

# Application
VITE_APP_NAME=SOMA
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=test

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=5000

# Feature Flags
VITE_FEATURE_DEBUG_MODE=true
VITE_FEATURE_MOCK_DATA=true
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_ERROR_REPORTING=false

# LLM Configuration
VITE_LLM_DEFAULT_MODEL=mock
VITE_LLM_MAX_TOKENS=512
VITE_LLM_TEMPERATURE=0

# Logging
VITE_LOG_LEVEL=error
VITE_LOG_TO_FILE=false

# Performance
VITE_ENABLE_PROFILING=false
VITE_ENABLE_SOURCE_MAPS=true

# 3D Renderer Settings
VITE_3D_ANTIALIAS=false
VITE_3D_PIXEL_RATIO=1
VITE_3D_MAX_FPS=30
