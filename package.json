{
  "name": "soma-kimi",
  "version": "1.0.0",
  "description": "Understand your body like a physician understands their patients",
  "type": "module",
  "scripts": {
    "cli": "tsx cli.ts",
    "demo": "tsx demo.ts",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production && npx vite-bundle-visualizer",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src core --ext .ts,.tsx",
    "lint:fix": "eslint src core --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"core/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:build:macos": "tauri build --target universal-apple-darwin",
    "tauri:build:macos-intel": "tauri build --target x86_64-apple-darwin",
    "tauri:build:macos-arm": "tauri build --target aarch64-apple-darwin",
    "tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:linux-arm": "tauri build --target aarch64-unknown-linux-gnu",
    "tauri:ios": "tauri ios dev",
    "tauri:ios:build": "tauri ios build",
    "tauri:android": "tauri android dev",
    "tauri:android:build": "tauri android build",
    "release": "npm run release:check && npm run release:build",
    "release:check": "npm run typecheck && npm run test:run",
    "release:build": "npm run build:prod && npm run tauri:build",
    "release:macos": "npm run release:check && npm run build:prod && npm run tauri:build:macos",
    "release:windows": "npm run release:check && npm run build:prod && npm run tauri:build:windows",
    "release:linux": "npm run release:check && npm run build:prod && npm run tauri:build:linux",
    "version:patch": "npm version patch && npm run version:sync",
    "version:minor": "npm version minor && npm run version:sync",
    "version:major": "npm version major && npm run version:sync",
    "version:sync": "node scripts/sync-version.js",
    "changelog": "node scripts/generate-changelog.js",
    "clean": "rimraf dist src-tauri/target node_modules/.vite",
    "clean:all": "rimraf dist src-tauri/target node_modules/.vite node_modules",
    "prepare": "husky || true",
    "icons:generate": "node scripts/generate-icons.js"
  },
  "keywords": [
    "health",
    "education",
    "privacy",
    "local-first",
    "open-source",
    "llm",
    "medical"
  ],
  "author": "Danny Gomez",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannygomez/biological-self.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "bugs": {
    "url": "https://github.com/dannygomez/biological-self/issues"
  },
  "homepage": "https://github.com/dannygomez/biological-self#readme",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lightningcss": "^1.31.1",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "playwright": "^1.58.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "sharp": "^0.33.0",
    "terser": "^5.30.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-bundle-visualizer": "^1.2.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@lancedb/lancedb": "^0.23.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.9.6",
    "@types/three": "^0.182.0",
    "@xenova/transformers": "^2.17.2",
    "apache-arrow": "^18.1.0",
    "better-sqlite3": "^12.6.2",
    "chromadb": "^3.2.2",
    "ollama": "^0.6.3",
    "pdf-parse": "^2.4.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0",
    "undici": "^7.18.2",
    "uuid": "^13.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
